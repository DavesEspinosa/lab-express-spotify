{{#each artist}}
<section class="magic">
<div class="card" style="width: 18rem;">
    {{#if images.[0].url}}
  <img src="{{ images.[0].url }}" class="card-img-top" alt="...">
  {{else}}
  <img src="/images/3604.jpg" class="card-img-top" alt="...">
  {{/if}}
  <div class="card-body">
    <h5 class="card-title">{{ name}}</h5>
    <a href="/albums/{{id}}" class="btn btn-primary">View albums</a>
  </div>
</div>
</section>
{{/each}}